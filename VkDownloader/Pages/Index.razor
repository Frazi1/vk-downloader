@page "/"
@using VkDownloader.Vk
@inject VkCredentialsStorage VkCredentialsStorage;

<h1>Hello, world!</h1>

Welcome to your new app.

<div>
    @if (VkCredentialsStorage.GetAccessToken() == null)
    {
        <VkAuthComponent/>
    }
    else
    {
        <span>You are authenticated</span>
    }
</div>